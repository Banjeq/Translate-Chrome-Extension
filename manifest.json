{
    "manifest_version": 2,

    "name": "Translate",
    "version": "1.3.3.7",

    "icons": {
        "16": "Icon-16.png",
        "32": "Icon-32.png",
        "48": "Icon-48.png",
        "128": "Icon-128.png"
    },

	"permissions": ["storage", "tabs", "<all_urls>", "fontSettings", "contextMenus", "activeTab"],
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	
	
    "browser_action": {
        "default_title": "Перевод",
       "default_icon": "Icon-48.png",
        "default_popup": "popup.html"
    }
 }